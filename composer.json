{
    "name": "thedevs-cz/wms",
    "type": "project",
    "description": "",
    "authors": [
        {
            "name": "Jan Mikeš",
            "email": "j.mikes@me.com"
        },
        {
            "name": "Lukáš Huňka",
            "email": "lukas@hunka.cz"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-simplexml": "*",
        "ext-uuid": "*",
        "api-platform/doctrine-orm": "^4.0",
        "api-platform/symfony": "^4.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^3.0",
        "endroid/qr-code-bundle": "^5.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "knplabs/knp-snappy-bundle": "^1.10",
        "knplabs/knp-time-bundle": "^2.2",
        "monolog/monolog": "^3.0",
        "nelmio/cors-bundle": "^2.4",
        "nette/utils": "^4.0",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.33",
        "psr-discovery/cache-implementations": "^1.1",
        "psr-discovery/container-implementations": "^1.1",
        "psr-discovery/event-dispatcher-implementations": "^1.1",
        "psr-discovery/http-client-implementations": "^1.1",
        "ramsey/uuid": "^4.2",
        "ramsey/uuid-doctrine": "^2.0",
        "runtime/frankenphp-symfony": "^0.2.0",
        "sentry/sentry-symfony": "^5.0",
        "symfony/asset": "^7.0",
        "symfony/asset-mapper": "7.*",
        "symfony/config": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/doctrine-messenger": "^7.0",
        "symfony/dom-crawler": "7.*",
        "symfony/dotenv": "^7.0",
        "symfony/expression-language": "7.*",
        "symfony/flex": "^2.1",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/http-client-contracts": "^3.4",
        "symfony/http-kernel": "^7.0",
        "symfony/intl": "^7.0",
        "symfony/mailer": "^7.0",
        "symfony/messenger": "^7.0",
        "symfony/mime": "^7.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/process": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/security-csrf": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/string": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/ux-live-component": "^2.19",
        "symfony/ux-turbo": "^2.18",
        "symfony/validator": "^7.0",
        "symfony/web-link": "^7.0",
        "symfony/yaml": "^7.0",
        "symfonycasts/dynamic-forms": "^0.1.2",
        "twig/cssinliner-extra": "^3.10",
        "twig/extra-bundle": "^3.10",
        "twig/inky-extra": "^3.10",
        "twig/intl-extra": "^3.8",
        "twig/string-extra": "^3.7",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpunit/phpunit": "^11.0",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/debug-bundle": "^7.0",
        "symfony/maker-bundle": "^1.61",
        "symfony/web-profiler-bundle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "TheDevs\\WMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheDevs\\WMS\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan --memory-limit=-1 analyse",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.*",
            "docker": false
        }
    },
    "replace": {
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-uuid": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "endroid/installer": true,
            "php-http/discovery": false,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    }
}
